{
  "CustomSettings": {
    "AppName": "Sympli Searching API",
    "Version": "1.0",
    "EnableFeatureX": true
  },
  "GoogleSearch": {
    "Url": "https://www.google.com",
    "Limit": 40
  },
  "BingSearch": {
    "Url": "https://www.bing.com",
    "Limit": 40
  },
  "CacheingExpireation": 60, // in minutes
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
