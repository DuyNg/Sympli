{
  "CustomSettings": {
    "AppName": "Sympli Searching API",
    "Version": "1.0",
    "EnableFeatureX": true
  },
  "GoogleSearch": {
    "Url": "https://www.google.com",
    "Limit": 100
  },
  "BingSearch": {
    "Url": "https://www.bing.com",
    "Limit": 100
  },
  "CacheingExpireation": 60, // in minutes
  "AllowedOrigins": [
    "http://localhost:3000",
    "https://example.com"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
